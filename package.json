{
  "name": "mantra-demo",
  "version": "0.4.1",
  "scripts": {
    "lint": "eslint ./lib ./client ./server --ext .js",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9001 -s ./public"
  },
  "devDependencies": {
    "@kadira/storybook": "1.22.1",
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "^1.1.0",
    "chai": "3.x.x",
    "enzyme": "^2.2.0",
    "eslint": "1.10.x",
    "eslint-plugin-react": "3.15.x",
    "jsdom": "^8.0.4",
    "mocha": "2.x.x",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.0.0",
    "sinon": "1.17.x",
    "style-loader": "^0.13.1",
    "autoprefixer": "^6.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "domready": "^1.0.8",
    "mantra-core": "^1.7.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-mounter": "^1.2.0",
    "react-swf": "^1.0.6"
  },
  "private": true,
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
